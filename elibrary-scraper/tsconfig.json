{
    "compilerOptions": {
        "strict": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "preserveConstEnums": true,
        "noEmit": true,
        "target": "es2015"
    },
    "include": [
        "*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}